{
  "expo": {
    "name": "Amara",
    "slug": "Amara",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/connect-logo.png",
    "scheme": "Amara",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/connect-logo.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "notification":{
        "icon":"./assets/images/connect-logo.png",
        "color":"#ffffff"
      }
    },
    "web": {
      "bundler":"metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/connect-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra":{
      "router":{},
      "eas": {
        "projectId": "523f070e-e4fa-43fa-9c17-4fb2a4c303b5"
      }
    }
  }
}
