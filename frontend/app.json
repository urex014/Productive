{
  "expo": {
    "name": "Amara",
    "slug": "Amara",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/connect-logo.png",
    "scheme": "Amara",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/connect-logo.png",
        "backgroundColor": "#000000",
        "googleServicesFile": "./android/app/google-services.json"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Amara"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/connect-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/connect-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "523f070e-e4fa-43fa-9c17-4fb2a4c303b5"
      }
    }
  }
}
